{
    "name": "sdh100shaun/cognito-poc",
    "description": "A proof of concept for PHP authentication using cognito",
    "type": "project",
    "require": {
        "aws/aws-sdk-php": "^3.172",
        "php-di/php-di": "^6.1",
        "slim/psr7": "dev-master",
        "slim/slim": "^4.0",
        "slim/twig-view": "^3.0",
        "vlucas/phpdotenv": "5.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shaun Hare",
            "email": "shaun@shaunhare.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
